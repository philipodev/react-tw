{
  "name": "react-tw",
  "version": "1.0.0",
  "type": "module",
  "files": ["dist"],
  "main": "./dist/react-tw.cjs.js",
  "module": "./dist/react-tw.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-tw.es.js",
      "require": "./dist/react-tw.cjs.js"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest run src",
    "test:watch": "vitest src",
    "coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.38.0",
    "jsdom": "^21.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vitest": "^0.30.1"
  }
}
